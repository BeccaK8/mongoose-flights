<%- include('../partials/header') %>

<table>
    <thead>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No</th>
        <th>Departs</th>
    </thead>
</table>

<% flights.forEach(function(f) { %>
    <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
    </tr>
<% }) %>

<%- include('../partials/footer') %>